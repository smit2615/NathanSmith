@{
    ViewBag.Title = "Resume";
}

<link rel="stylesheet" href="~/Content/Resume.css" />

<h1>My Resume</h1>
<div id="sm-device-info" class="row visible-xs-block">
    <i>**It looks like you're viewing on a small device... click the button to view my resume!</i>
</div>
<div>
    <a id="resume_download"class="btn btn-info" href="~/Resources/Resume.pdf" onclick="ga('send', 'event', 'Nathan Smith Resume', 'click');">
    <span class="glyphicon glyphicon-download"></span> Download</a>
</div>
<div class="container-fluid">
    <object class="resume hidden-xs" data="~/Resources/Resume.pdf"/>
</div>

<script src="~/Scripts/Resume.js"></script>
